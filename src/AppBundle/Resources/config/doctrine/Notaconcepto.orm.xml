<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Notaconcepto" table="NotaConcepto">
    <id name="idnotaconcepto" type="integer" column="idNotaConcepto">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreconcepto" type="string" column="nombreConcepto" length="45" nullable="true"/>
    <field name="nomenclatura" type="string" column="nomenclatura" length="2" nullable="true"/>
    <field name="notamin" type="integer" column="notaMin" nullable="true"/>
    <field name="notamax" type="integer" column="notaMax" nullable="true"/>
  </entity>
</doctrine-mapping>
